<?xml version="1.0" encoding="UTF-8"?>
<web-app
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://xmlns.jcp.org/xml/ns/javaee"
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
 
<welcome-file-list>
    <welcome-file>PackageManage.jsp</welcome-file>
</welcome-file-list>



<!-- 프런트 컨트롤러 서블릿 등록 -->
<servlet>
    <servlet-name>AuthorityController</servlet-name>
    <servlet-class>Sogong.IMS.controller.AuthorityManagement.AuthorityController</servlet-class>
</servlet>
 
<!-- URL패턴 맵핑-->
<!-- <servlet-mapping>
    <servlet-name>AuthorityController</servlet-name>
    <url-pattern>/AuthorityManage/*.do</url-pattern>
</servlet-mapping> -->

<servlet>
    <servlet-name>AuthorityManage</servlet-name>
    <jsp-file>/AuthorityManage.jsp</jsp-file>
</servlet>
<servlet-mapping>
    <servlet-name>AuthorityManage</servlet-name>
    <url-pattern>/AuthorityManage</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>packageManage</servlet-name>
    <jsp-file>/PackageManage.jsp</jsp-file>
</servlet>
<servlet-mapping>
    <servlet-name>packageManage</servlet-name>
    <url-pattern>/PackageManage</url-pattern>
</servlet-mapping>
</web-app>